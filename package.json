{
  "name": "json-to-dart-3",
  "displayName": "JSON to Dart Model Generator",
  "description": "Quickly convert JSON to Dart model classes with fromJson and toJson methods for Flutter projects",
  "version": "0.0.1",
  "publisher": "PhucChiVas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PhucChiVas161/json-to-dart-3"
  },
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Programming Languages",
    "Formatters"
  ],
  "keywords": [
    "dart",
    "flutter",
    "json",
    "model",
    "generator",
    "converter"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "json-to-dart-3.convertJsonToDart",
        "title": "JSON to Dart Model"
      }
    ],
    "configuration": {
      "title": "JSON to Dart",
      "properties": {
        "jsonToDart.language": {
          "type": "string",
          "default": "en",
          "enum": [
            "en",
            "vi"
          ],
          "enumDescriptions": [
            "English",
            "Tiếng Việt"
          ],
          "description": "Extension language / Ngôn ngữ extension"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.46.3",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}